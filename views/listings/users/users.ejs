<%- layout('/layouts/boilerplate.ejs') %>

<div class="container mt-5">
    <h2 class="text-center">All Registered Users</h2>
    <table class="table table-striped mt-3">
        <thead>
            <tr>
                <th>Username</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            <% users.forEach(user => { %>
                <tr>
                    <td><%= user.username %></td>
                    <td><%= user.email %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</div>
